<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:paddingLeft="16dp"
    android:paddingRight="16dp"
    >
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="@string/desc"
        />

    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tableLayout1"
        android:layout_centerInParent="true"
        android:layout_marginLeft="16dp"
        android:gravity="center">
        <TableRow
            android:id="@+id/tableRow1"
            android:layout_marginBottom="16dp"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <Button android:id="@+id/eth2"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:textColor="#f14314"
                android:text="Pharao"/>
            <Button android:id="@+id/blue2"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:textColor="#3c6ff0"
                android:text="Blau links"/>
            <Button android:id="@+id/blue1"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:textColor="#3c6ff0"
                android:text="Blau rechts"/>
        </TableRow>
        <TableRow
            android:id="@+id/tableRow2"
            android:layout_marginBottom="16dp"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <Button android:id="@+id/mylight4"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:textColor="#2cff7a"
                android:text="Boden"/>
            <Button android:id="@+id/red1"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:textColor="#ff3232"
                android:text="Rot links"/>
            <Button android:id="@+id/red2"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:textColor="#ff3232"
                android:text="Rot rechts"/>
        </TableRow>
    </TableLayout>
    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tableLayout2"
        android:layout_centerInParent="true"
        android:layout_marginLeft="16dp"
        android:gravity="center">
        <TableRow
            android:id="@+id/tableRow3"
            android:layout_marginBottom="16dp"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <Button android:id="@+id/mylight15"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:background="#000000"
                android:textColor="#888888"
                android:text="Bett"/>
            <Button android:id="@+id/eth1"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:background="#000000"
                android:textColor="#888888"
                android:text="TV"/>
            <Button android:id="@+id/mylight14"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:background="#000000"
                android:textColor="#888888"
                android:text="Mirror"/>
        </TableRow>
        <TableRow
            android:id="@+id/tableRow4"
            android:layout_marginBottom="16dp"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <Button android:id="@+id/na1"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:background="#000000"
                android:textColor="#888888"
                android:text="EPL"/>
            <Button android:id="@+id/mylight8d"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:background="#000000"
                android:textColor="#888888"
                android:text="TV 10s"/>
            <Button android:id="@+id/zsun1"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:background="#000000"
                android:textColor="#888888"
                android:text="Decke"/>
        </TableRow>
    </TableLayout>
    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tableLayout2"
        android:layout_centerInParent="true"
        android:layout_marginLeft="16dp"
        android:gravity="center">
        <TableRow
            android:id="@+id/tableRow5"
            android:layout_marginBottom="16dp"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">
            <Button android:id="@+id/na2"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:background="#000000"
                android:textColor="#888888"
                android:text="TXT"/>
            <Button android:id="@+id/jarbot"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:background="#000000"
                android:textColor="#888888"
                android:text="JarBot"/>
            <Button android:id="@+id/zsun1d"
                android:layout_marginBottom="16dp"
                android:layout_marginRight="16dp"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:background="#000000"
                android:textColor="#888888"
                android:text="Decke 10s"/>
        </TableRow>
    </TableLayout>

    <TextView android:id="@+id/volt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        android:text=""/>

</LinearLayout>

